# platform :ios, '13.0'

target 'Chat_App' do
  use_frameworks!
  platform :ios, '13.0'

 # Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Core'

  # Facebook Sign In
  pod 'FBSDKLoginKit'

  # Google Sign In
  pod 'GoogleSignIn'
end
