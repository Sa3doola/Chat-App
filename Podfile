# Uncomment the next line to define a global platform for your project
platform :ios, '13.7'

target 'Chat_App' do
  use_frameworks!

 # Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'

  # Facebook Sign In
  pod 'FBSDKLoginKit'

  # Google Sign In
  pod 'GoogleSignIn'

  pod 'MessageKit'
  pod 'JGProgressHUD'
  pod 'RealmSwift'
  pod 'SDWebImage'
end
